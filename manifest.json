{
    "manifest_version": 3,
    "name": "Accessibility Extension",
    "description": "Extension to improve accessibility on the web",
    "version": "1.0.1",
    "API_KEY": "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",
  

  
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["script.js"],
        "css": ["style.css"]
      }
    ],
  
    "action": {
      "default_popup": "index.html",
      "default_icon": "icon.png"
    },
  
    "permissions": [
      "activeTab",
      "tabs",
      "declarativeContent",
      "storage",
      "unlimitedStorage",
      "downloads",
      "scripting"
    ]
  }